-- // Variable
local AutoCast = false

-- // Toggle WindUI
Tabs.Farm:Toggle({
    Title = "Auto Cast",
    Value = false,
    Callback = function(state)
        AutoCast = state
    end
})

-- // Services
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- // Get Rod Tool
local function GetRodTool()
    local char = LocalPlayer.Character
    if not char then return nil end
    for _, v in pairs(char:GetChildren()) do
        if v:IsA("Tool") and v.Name:match("Rod") then
            return v
        end
    end
    return nil
end

-- // Cast Function
local function CastRod()
    local tool = GetRodTool()
    if not tool then return end
    local events = tool:FindFirstChild("events")
    if not events then return end
    local cast = events:FindFirstChild("cast")
    if not cast then return end
    cast:FireServer(82.9, 1)
end

-- // Loop
task.spawn(function()
    while true do
        task.wait(0.01)
        if AutoCast then
            pcall(CastRod)
        end
    end
end)

-- Services
local Players = game:GetService("Players")
local VirtualInputManager = game:GetService("VirtualInputManager")
local GuiService = game:GetService("GuiService")

-- Locals
local LocalPlayer = Players.LocalPlayer
local PlayerGUI = LocalPlayer:WaitForChild("PlayerGui")

-- Config
local autoShake = false -- toggle auto shake
local autoShakeDelay = 0.1 -- delay antar tekan (default)

-- WindUI Toggle untuk Auto Shake
Tabs.Farm:Toggle({ 
    Title = "Auto Shake", 
    Value = false,
    Callback = function(state)
        autoShake = state
    end
})

-- WindUI Input untuk Speed / Delay
Tabs.Farm:Input({ 
    Title = "Speed Shake Set", 
    Value = tostring(autoShakeDelay),
    Placeholder = "Enter delay (seconds)",
    Callback = function(input)
        local delayValue = tonumber(input)
        if delayValue and delayValue > 0 then
            autoShakeDelay = delayValue
        end
    end
})

-- Auto Shake Logic (KeyCodeEvent Only)
PlayerGUI.ChildAdded:Connect(function(GUI)
    if GUI:IsA("ScreenGui") and GUI.Name == "shakeui" then
        local safezone = GUI:WaitForChild("safezone", 5)
        if safezone then
            safezone.ChildAdded:Connect(function(child)
                if child:IsA("ImageButton") and child.Name == "button" and child.Visible then
                    if autoShake then
                        task.wait(autoShakeDelay)

                        -- Jalankan loop auto tekan ENTER
                        task.spawn(function()
                            while autoShake and child.Parent == safezone and child.Visible do
                                pcall(function()
                                    GuiService.SelectedObject = child
                                    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Return, false, game)
                                    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.Return, false, game)
                                end)
                                task.wait(autoShakeDelay)
                            end
                            -- Reset SelectedObject kalau berhenti
                            GuiService.SelectedObject = nil
                        end)
                    end
                end
            end)
        end
    end
end)


