    Title = "Trading",
    Box = true,
    BoxBorder = true,
    Opened = true,
})

GameData.DataReplion = GameData.ReplionClient:WaitReplion("Data")
GameData.TierList = GameData.TierList001

for num, name in pairs(GameData.TierMap001) do
	GameData.nameToTier[name] = num
end

-- Normalize helper
function GameData.normalize(value)
	if not value then return {} end
	if type(value) == "string" then
		return { value }
	end

	local out = {}
	for k, v in pairs(value) do
		if type(k) == "string" and v == true then
			table.insert(out, k)
		elseif type(v) == "string" then
			table.insert(out, v)
		end
	end
	return out
end

-- Check if item tier matches selected tiers
function GameData.isTierMatch(itemTier, selected)
	for _, tName in ipairs(selected) do
		if GameData.nameToTier[tName] == tonumber(itemTier) then
			return true
		end
	end
	return false
end

-- Safe invoke trade request
function GameData.safeTrade(userid, uuid)
	pcall(function()
		GameData.TradeRemote:InvokeServer(userid, uuid)
	end)
end

-- Get fish list from inventory dengan jumlah
function GameData.getFishList()
	local inventory = GameData.DataReplion:GetExpect({ "Inventory", "Items" }) or {}
	local fishCount = {} -- Menyimpan jumlah per jenis ikan
	
	-- Hitung jumlah per jenis ikan
	for _, item in ipairs(inventory) do
		local ok, data = pcall(function()
			return GameData.ItemUtility.GetItemDataFromItemType("Items", item.Id)
		end)

		if ok and data and data.Data and data.Data.Type == "Fish" then
			local fishName = data.Data.Name
			fishCount[fishName] = (fishCount[fishName] or 0) + 1
		end
	end
	
	-- Konversi ke format yang diinginkan
	local list = {}
	for fishName, count in pairs(fishCount) do
		table.insert(list, {
			name = fishName,
			display = fishName .. " [" .. count .. "]",
			count = count
		})
	end
	
	-- Sort berdasarkan nama ikan
	table.sort(list, function(a, b)
		return a.name < b.name
	end)
	
	return list, fishCount
end

-- Worker function for auto trade
function GameData.workerTrade()
	GameData.RATE = 0.15
	GameData.LOOP_DELAY = 1

	while GameData.state.Auto2Trade do
		if GameData.state.stopTrade then
			return
		end

		local target = GameData.Players:FindFirstChild(GameData.state.targetPlayer)
		if not target then
			GameData.state.Auto2Trade = false
			WindUI:Notify({
				Title = "Trade",
				Content = "Target player not found, stopping trade.",
				Duration = 4,
				Icon = "error"
			})
			return
		end

		local inventory = GameData.DataReplion:GetExpect({ "Inventory", "Items" }) or {}
		local list = {}

		for _, item in ipairs(inventory) do
			local ok, data = pcall(function()
				return GameData.ItemUtility.GetItemDataFromItemType("Items", item.Id)
			end)

			if ok and data and data.Data then
				if data.Data.Type == "Fish" then
					local pass = false
					
					-- Check fish filter (prioritas jika diaktifkan)
					if GameData.state.useFishFilter and #GameData.state.selectedFish > 0 then
						for _, fishName in ipairs(GameData.state.selectedFish) do
							-- Extract nama ikan dari format "Granmaja [3]"
							local cleanFishName = fishName:match("^(.+) %[%d+%]$") or fishName
							-- Skip jika "None Fish"
							if cleanFishName ~= "None Fish" and data.Data.Name == cleanFishName then
								pass = true
								break
							end
						end
					else

						pass = GameData.isTierMatch(
							data.Data.Tier,
							GameData.state.selectedTiers
						)
					end
					
					if pass then
						table.insert(list, item.UUID)
					end
				end
			end
		end

		if #list == 0 then
			WindUI:Notify({
				Title = "Trade",
				Content = "No more fish for selected filter",
				Duration = 4,
				Icon = "fish"
			})
			GameData.state.Auto2Trade = false
			break
		end

		for _, uuid in ipairs(list) do
			if not GameData.state.Auto2Trade then
				break
			end

			GameData.safeTrade(target.UserId, uuid)
			task.wait(GameData.RATE)
		end

		task.wait(GameData.LOOP_DELAY)
	end
end

GameData.notifGui = GameData.player.PlayerGui:WaitForChild("Text Notifications")
GameData.notifFrame = GameData.notifGui:WaitForChild("Frame")

function GameData.checkTradeTile(tile)
	local header = tile:FindFirstChild("Header")
	if header and header:IsA("TextLabel") then
		if header.Text == "Trade completed!" then
			GameData.state.fishCount = GameData.state.fishCount + 1

			WindUI:Notify({
				Title = "Trade",
				Content = "Trade completed! (" .. GameData.state.fishCount .. "/" .. GameData.state.fishTarget .. ")",
				Duration = 3,
				Icon = "trade"
			})

			if GameData.state.fishTarget > 0 and GameData.state.fishCount >= GameData.state.fishTarget then
				GameData.state.Auto2Trade = false
				GameData.state.stopTrade = true

				WindUI:Notify({
					Title = "Trade",
					Content = "Trade target reached!",
					Duration = 3,
					Icon = "success"
				})
			end
		end
	end
end

for _, tile in ipairs(GameData.notifFrame:GetChildren()) do
	if tile.Name == "TextTile" then
		GameData.checkTradeTile(tile)
	end
end

GameData.notifFrame.ChildAdded:Connect(function(child)
	if child.Name == "TextTile" then
		local headerObj = child:WaitForChild("Header", 5)
		if headerObj then
			GameData.checkTradeTile(child)
		end
	end
end)


GameData.PlayerMap = {}

GameData.RefreshPlayers = function(dropdown)
	local options = {}
	table.clear(GameData.PlayerMap)

	for _, plr in ipairs(GameData.Players:GetPlayers()) do
		if plr ~= GameData.player then
			local label = plr.DisplayName .. " @" .. plr.Name
			GameData.PlayerMap[label] = plr.Name
			table.insert(options, label)
		end
	end

	dropdown:Refresh(options)

	if #options > 0 then
		dropdown:Select(options[1])
		GameData.state.targetPlayer = GameData.PlayerMap[options[1]]
	end
end

GameData.PlayerDropdown = Sec.Trading1:Dropdown({
	Title = "Select Target Player",
	Flag = "SelectTargetPlayer",
	SearchBarEnabled = true,
	Desc = "Choose a player to trade with",
	Values = {},
	Value = nil,
	Multi = false,
	AllowNone = false,
	Callback = function(selection)
		if GameData.PlayerMap[selection] then
			GameData.state.targetPlayer = GameData.PlayerMap[selection]
			WindUI:Notify({
				Title = "Target",
				Content = "Selected: " .. selection,
				Duration = 3,
				Icon = "user"
			})
		end
	end
})

GameData.RefreshPlayers(GameData.PlayerDropdown)

Sec.Trading1:Button({
	Title = "Refresh Player List",
	Desc = "Update the player dropdown",
	Callback = function()
		GameData.RefreshPlayers(GameData.PlayerDropdown)
	end
})

Sec.Trading1:Input({
	Title = "Fish Amount Trade",
	Flag = "FishAmountTrade",
	Desc = "Enter the number of fish to trade",
	Value = tostring(GameData.state.fishTarget or ""),
	Placeholder = "Example: 10",
	Type = "Input",
	Callback = function(input)
		local amount = tonumber(input)
		if amount then
			GameData.state.fishTarget = amount
			GameData.state.fishCount = 0
			WindUI:Notify({
				Title = "Trade",
				Content = "Trade target set: " .. amount,
				Duration = 3,
				Icon = "fish"
			})
		else
			WindUI:Notify({
				Title = "Error",
				Content = "Input must be a number!",
				Duration = 3,
				Icon = "error"
			})
		end
	end
})

-- Tier selection dropdown
Sec.Trading1:Dropdown({
	Title = "Select Tier(s)",
	Flag = "SelectTiers",
	Desc = "Choose one or more tiers",
	Values = GameData.TierList001,
	Value = { "Common" },
	Multi = true,
	AllowNone = true,
	Callback = function(selectedTiers)
		GameData.state.selectedTiers = GameData.normalize(selectedTiers)
		GameData.state.useFishFilter = false -- Nonaktifkan fish filter saat tier dipilih
		WindUI:Notify({
			Title = "Tier Selected",
			Content = table.concat(GameData.state.selectedTiers, ", "),
			Duration = 3,
			Icon = "star"
		})
	end
})

local FishDropdown
local fishListData = {} 

local function refreshFishDropdown()
	local fishList, fishCount = GameData.getFishList()
	local displayValues = {}
	fishListData = {}

	table.insert(displayValues, "None Fish")
	fishListData["None Fish"] = "None Fish"
	
	for _, fishData in ipairs(fishList) do
		table.insert(displayValues, fishData.display)
		fishListData[fishData.display] = fishData.name
	end
	
	FishDropdown:Refresh(displayValues)
	
	FishDropdown:Select({"None Fish"})
	
	local totalFish = 0
	for _, count in pairs(fishCount) do
		totalFish = totalFish + count
	end
	
	return #fishList, totalFish
end

FishDropdown = Sec.Trading1:Dropdown({
	Title = "Select Fish",
	Flag = "SelectFish",
    SearchBarEnabled = true,
	Values = {},
	Value = {},
	Multi = true,
	AllowNone = true,
	Callback = function(selectedFish)

		local cleanSelected = {}
		for _, displayName in ipairs(GameData.normalize(selectedFish)) do
			local cleanName = fishListData[displayName] or displayName:match("^(.+) %[%d+%]$") or displayName
			table.insert(cleanSelected, cleanName)
		end
		
		GameData.state.selectedFish = cleanSelected
		GameData.state.useFishFilter = false
		
		for _, fishName in ipairs(cleanSelected) do
			if fishName ~= "None Fish" then
				GameData.state.useFishFilter = true
				break
			end
		end
		
		if not GameData.state.useFishFilter then
			GameData.state.selectedFish = {}
		end
		
		WindUI:Notify({
			Title = "Fish Filter",
			Content = GameData.state.useFishFilter and 
				("Selected " .. (#cleanSelected - (cleanSelected[1] == "None Fish" and 1 or 0)) .. " fish types") or 
				"Fish filter disabled (None Fish selected)",
			Duration = 3,
			Icon = "fish"
		})
	end
})

-- Refresh fish list button
Sec.Trading1:Button({
	Title = "Refresh Fish List",
	Desc = "Update available fish from inventory (auto-selects None Fish)",
	Callback = function()
		local fishTypes, totalFish = refreshFishDropdown()
		
		GameData.state.selectedFish = {}
		GameData.state.useFishFilter = false
		
		WindUI:Notify({
			Title = "Fish List",
			Content = fishTypes .. " fish types (" .. totalFish .. " total fish)\nAuto-selected: None Fish",
			Duration = 4,
			Icon = "refresh"
		})
	end
})

-- Auto trade toggle
Sec.Trading1:Toggle({
	Title = "Auto Trade",
	Flag = "AutoTrade",
	Desc = "Enable or disable auto trading",
	Value = false,
	Callback = function(state)
		GameData.state.Auto2Trade = state
		GameData.state.stopTrade = not state

		if state then
			task.spawn(GameData.workerTrade)
		end

		WindUI:Notify({
			Title = "Auto Trade",
			Content = state and "Auto Trade Enabled" or "Auto Trade Disabled",
			Duration = 3,
			Icon = "trade"
		})
	end
})

