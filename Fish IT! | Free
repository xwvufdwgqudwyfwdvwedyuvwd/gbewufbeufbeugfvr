    Title = "Completed Delay",
    Flag = "CompletedDelayv1",
    Desc = "Delay after fishing completed",
    Value = "1",
    InputIcon = "clock",
    Type = "Input",
    Placeholder = "Default: 1",
    Callback = function(input)
        GameData.completedDelay = tonumber(input) or 1
    end
})

