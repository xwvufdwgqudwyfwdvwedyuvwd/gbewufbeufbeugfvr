    Title = "Cast Delay",
    Flag = "CastDelayv1",
    Desc = "Delay before casting fishing rod",
    Value = "1.5",
    InputIcon = "clock",
    Type = "Input",
    Placeholder = "Default: 1.5",
    Callback = function(input)
        GameData.castDelay = tonumber(input) or 1.5
    end
})

