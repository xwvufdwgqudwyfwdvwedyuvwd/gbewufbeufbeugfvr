        local purchaseRemote = ReplicatedStorage
            :WaitForChild("Packages")
            :WaitForChild("_Index")
            :WaitForChild("sleitnick_net@0.2.0")
            :WaitForChild("net")
            :WaitForChild("RF/PurchaseBait")

        for _, id in ipairs(selectedBaits) do
            pcall(function()
                purchaseRemote:InvokeServer(id)
            end)
        end
    end,
}, "PurchaseBaitsButton")

