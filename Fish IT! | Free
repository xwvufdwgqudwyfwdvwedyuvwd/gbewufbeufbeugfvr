    Name = "Bypass Radar",
    Default = false,
    Callback = function(state)
        Window.AlwayShowTab = state

        local remoteFunc = ReplicatedStorage
            :WaitForChild("Packages")
            :WaitForChild("_Index")
            :WaitForChild("sleitnick_net@0.2.0")
            :WaitForChild("net")
            :WaitForChild("RF/UpdateFishingRadar")

        pcall(function()
            remoteFunc:InvokeServer(state)
        end)
    end,
}, "BypassRadarToggle")

