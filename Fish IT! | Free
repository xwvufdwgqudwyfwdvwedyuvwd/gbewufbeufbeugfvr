    Value = "10",
    InputIcon = "shield",
    Type = "Input",
    Placeholder = "Default: 10",
    Callback = function(input)
        local value = tonumber(input)
        if value then
            GameData.antiStuckTime = value
        end
    end
})

Sec.Main3 = Tabs.Main:Section({
    Title = "Animation Fishing",
    Box = true,
    BoxBorder = true,
    Opened = false,
})

GameData.Player099 = GameData.Services099.Players099.LocalPlayer

-- Reference remote event
GameData.REFishCaught099 =
    GameData.Services099.ReplicatedStorage099
        .Packages._Index["sleitnick_net@0.2.0"]
        .net["RE/FishCaught"]

-- Animations folder
GameData.AnimationsFolder099 = GameData.Services099.ReplicatedStorage099
    :WaitForChild("Modules")
    :WaitForChild("Animations")

-- AnimationController
GameData.AnimationController099 =
    require(GameData.Services099.ReplicatedStorage099.Controllers.AnimationController)

-- Override PlayAnimation
GameData.OldPlayAnimation099 = GameData.AnimationController099.PlayAnimation
GameData.AnimationController099.PlayAnimation = function(self, animName, ...)
    if GameData.AnimDisabled099 then
        return
    end
    return GameData.OldPlayAnimation099(self, animName, ...)
end

-- Load animations
for _, anim in ipairs(GameData.AnimationsFolder099:GetChildren()) do
    if anim:IsA("Animation") and anim.Name:find("FishCaught") then
        table.insert(GameData.AnimationList099, anim.Name)
        GameData.AnimationMap099[anim.Name] = anim.AnimationId
    end
end

-- Function play fish caught animation
function GameData:PlayFishCaught099()
    if not self.AnimationEnabled099 then return end
    if not self.SelectedAnimationId099 then return end

    self.Character099 = self.Player099.Character or self.Player099.CharacterAdded:Wait()
    if not self.Character099 then return end

    self.Humanoid099 = self.Character099:FindFirstChildOfClass("Humanoid")
    if not self.Humanoid099 then return end

    self.Animator099 = self.Humanoid099:FindFirstChildOfClass("Animator")
    if not self.Animator099 then
        self.Animator099 = Instance.new("Animator")
        self.Animator099.Parent = self.Humanoid099
    end

    if self.CurrentTrack099 then
        self.CurrentTrack099:Stop()
        self.CurrentTrack099:Destroy()
        self.CurrentTrack099 = nil
    end

    self.AnimationInstance099 = Instance.new("Animation")
    self.AnimationInstance099.AnimationId = self.SelectedAnimationId099

    self.CurrentTrack099 = self.Animator099:LoadAnimation(self.AnimationInstance099)
    self.CurrentTrack099:Play()
end

-- Connect remote event
GameData.REFishCaught099.OnClientEvent:Connect(function()
    GameData:PlayFishCaught099()
end)

Sec.Main3:Dropdown({
    Title = "Select Animation",
    Flag = "SelectAnimation",
    Desc = "Choose animation to load",
    Values = GameData.AnimationList099,
    Value = "",
    Multi = false,
    AllowNone = true,

    Callback = function(value)
        GameData.SelectedAnimationId099 = GameData.AnimationMap099[value]
    end
})

Sec.Main3:Toggle({
    Title = "Enable Animation",
    Flag = "EnableAnimation",
    Desc = "Enable / disable custom animation",
    Value = false,

    Callback = function(value)
        GameData.AnimationEnabled099 = value
        GameData.AnimDisabled099 = value

        if not value and GameData.CurrentTrack099 then
            GameData.CurrentTrack099:Stop()
            GameData.CurrentTrack099:Destroy()
            GameData.CurrentTrack099 = nil
        end

        if value then
            pcall(function()
                GameData.AnimationController099:DestroyActiveAnimationTracks()
            end)
        end
    end
})

Sec.Main4 = Tabs.Main:Section({
    Title = "Blatant",
    Box = true,
    BoxBorder = true,
    Opened = true,
})

-- SETUP NET
GameData.net = GameData.SVC.RS.Packages._Index["sleitnick_net@0.2.0"]:WaitForChild("net")

GameData.RF = {
    ChargeFishingRod = GameData.net:WaitForChild("RF/ChargeFishingRod"),
    RequestFishingMinigameStarted = GameData.net:WaitForChild("RF/RequestFishingMinigameStarted"),
    CancelFishing = GameData.net:WaitForChild("RF/CancelFishingInputs")
}

GameData.RE = {
    FishingCompleted = GameData.net:WaitForChild("RE/FishingCompleted"),
    FishCaught = GameData.net:WaitForChild("RE/FishCaught")
}

-- CUTSCENE CONTROLLER
do
    local ok, controller = pcall(function()
        return require(GameData.SVC.RS.Controllers.CutsceneController)
    end)

    if ok and controller then
        GameData.cutsceneController = controller
        GameData.originalPlay = controller.Play
        GameData.originalStop = controller.Stop
    end
end

-- ENABLE SKIP SYSTEM
GameData.EnableSkip = function()
    if GameData.cutsceneController then
        if GameData.cutsceneController.ReplicateCutscene then
            GameData.cutsceneController.ReplicateCutscene.OnClientEvent:Connect(function() end)
        end
        if GameData.cutsceneController.StopCutscene then
            GameData.cutsceneController.StopCutscene.OnClientEvent:Connect(function() end)
        end

        GameData.cutsceneController.Play = function() end
        GameData.cutsceneController.Stop = function() end
    end
end

GameData.EnableSkip()

-- GLOBAL CONFIG
_G.BlatantV2 = false
_G.DelayLoopStartV2 = 1.78     -- delay cast
_G.DelayCompleteV2 = 0.7       -- delay complete + cancel

-- AUTO CANCEL SAAT FISH CAUGHT
local lastCancel = 0
GameData.RE.FishCaught.OnClientEvent:Connect(function()
    if tick() - lastCancel < 0.001 then return end
    lastCancel = tick()

    pcall(function()
        GameData.RF.CancelFishing:InvokeServer()
    end)
end)

-- FASTEST FUNCTION
GameData.Fastest = function()
    task.spawn(function()
        task.wait(_G.DelayCompleteV2)
        pcall(function() GameData.RF.CancelFishing:InvokeServer() end)
        pcall(function() GameData.RF.ChargeFishingRod:InvokeServer(workspace:GetServerTimeNow()) end)
        pcall(function() GameData.RF.RequestFishingMinigameStarted:InvokeServer(-1, 0.999) end)

        task.wait(_G.DelayCompleteV2)
        pcall(function() GameData.RE.FishingCompleted:FireServer() end)
    end)
end

-- LOOP START
GameData.startBlatantV2 = function()
    task.spawn(function()
        while _G.BlatantV2 do
            GameData.Fastest()
            task.wait(_G.DelayLoopStartV2)
        end
    end)
end

Sec.Main4:Toggle({
    Title = "Blatant",
    Flag = "BlatantV1",
    Value = _G.BlatantV2,

    Callback = function(value)
        _G.BlatantV2 = value
        if value then
            GameData.startBlatantV2()
        end
    end
})

