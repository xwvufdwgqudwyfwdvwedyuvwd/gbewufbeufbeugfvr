task.spawn(function()
    while task.wait(0.2) do
        if not GameData.OverheadEnabledv3 then
            continue
        end

        GameData.GetSuccessv3 = pcall(function()
            GameData:GetOverheadv3()
        end)

        if not GameData.GetSuccessv3 then
            continue
        end

        if GameData.Headerv3 and GameData.CustomNamev3 ~= "" then
            GameData.Headerv3.Text = GameData.CustomNamev3
        end

        if GameData.LevelLabelv3 and GameData.CustomLevelv3 ~= "" then
            GameData.LevelLabelv3.Text = GameData.CustomLevelv3
        end
    end
end)

Sec.Player = Tabs.Player:Section({
    Title = "Utility",
    Box = true,
    BoxBorder = true,
    Opened = true,
})

function GameData:ResetCameraToPlayer()
	local char = self.Player.Character
	if not char then return end
	local head = char:FindFirstChild("Head") or char:FindFirstChild("HumanoidRootPart")
	if head then
		self.Camera.CFrame = head.CFrame
	end
end

function GameData:SetCharacterMove(enabled)
	local char = self.Player.Character
	if not char then return end
	local hum = char:FindFirstChildOfClass("Humanoid")
	if not hum then return end
	if enabled then
		hum.WalkSpeed = 16
		hum.JumpPower = 50
	else
		hum.WalkSpeed = 0
		hum.JumpPower = 50
	end
end

GameData.GUI = Instance.new("ScreenGui")
GameData.GUI.Name = "DroneCameraGUI"
GameData.GUI.ResetOnSpawn = false
GameData.GUI.Enabled = false
GameData.GUI.Parent = GameData.Player.PlayerGui

GameData.LeftFrame = Instance.new("Frame")
GameData.LeftFrame.Size = UDim2.fromScale(0.28, 0.35)
GameData.LeftFrame.Position = UDim2.fromScale(0.03, 0.55)
GameData.LeftFrame.AnchorPoint = Vector2.new(0, 1)
GameData.LeftFrame.BackgroundTransparency = 1
GameData.LeftFrame.Parent = GameData.GUI

function GameData:CreateButton(text, pos)
	local b = Instance.new("TextButton")
	b.Size = UDim2.fromScale(0.32, 0.32)
	b.Position = pos
	b.Text = text
	b.Font = Enum.Font.GothamBold
	b.TextScaled = true
	b.BackgroundColor3 = Color3.fromRGB(30,30,30)
	b.TextColor3 = Color3.new(1,1,1)
	b.Parent = self.LeftFrame
	local c = Instance.new("UICorner")
	c.CornerRadius = UDim.new(0.25,0)
	c.Parent = b
	return b
end

GameData.Buttons.Forward = GameData:CreateButton("▲", UDim2.fromScale(0.34, 0))
GameData.Buttons.Back    = GameData:CreateButton("▼", UDim2.fromScale(0.34, 0.68))
GameData.Buttons.Left    = GameData:CreateButton("◀", UDim2.fromScale(0, 0.34))
GameData.Buttons.Right   = GameData:CreateButton("▶", UDim2.fromScale(0.68, 0.34))

function GameData:EnableDrone()
	self.Drone = true
	self.Yaw = 0
	self.Pitch = 0
	self:SetCharacterMove(false)
	self.Camera.CameraType = Enum.CameraType.Scriptable
	self:ResetCameraToPlayer()
	self.GUI.Enabled = true
end

function GameData:DisableDrone()
	self.Drone = false
	self:SetCharacterMove(true)
	self.Camera.CameraType = Enum.CameraType.Custom
	self:ResetCameraToPlayer()
	self.GUI.Enabled = false
end

GameData.Buttons.Forward.MouseButton1Down:Connect(function()
	GameData.MoveForward = 1
end)
GameData.Buttons.Forward.MouseButton1Up:Connect(function()
	GameData.MoveForward = 0
end)

GameData.Buttons.Back.MouseButton1Down:Connect(function()
	GameData.MoveForward = -1
end)
GameData.Buttons.Back.MouseButton1Up:Connect(function()
	GameData.MoveForward = 0
end)

GameData.Buttons.Left.MouseButton1Down:Connect(function()
	GameData.MoveRight = -1
end)
GameData.Buttons.Left.MouseButton1Up:Connect(function()
	GameData.MoveRight = 0
end)

GameData.Buttons.Right.MouseButton1Down:Connect(function()
	GameData.MoveRight = 1
end)
GameData.Buttons.Right.MouseButton1Up:Connect(function()
	GameData.MoveRight = 0
end)

GameData.Services.UIS.TouchMoved:Connect(function(input, gpe)
	if not GameData.Drone or gpe then return end
	GameData.Yaw -= input.Delta.X * GameData.Sensitivity
	GameData.Pitch -= input.Delta.Y * GameData.Sensitivity
	GameData.Pitch = math.clamp(GameData.Pitch, -80, 80)
end)

GameData.Services.UIS.InputBegan:Connect(function(input, gpe)
	if gpe or not GameData.Drone then return end
	if input.KeyCode == Enum.KeyCode.W then
		GameData.MoveForward = 1
	elseif input.KeyCode == Enum.KeyCode.S then
		GameData.MoveForward = -1
	elseif input.KeyCode == Enum.KeyCode.A then
		GameData.MoveRight = -1
	elseif input.KeyCode == Enum.KeyCode.D then
		GameData.MoveRight = 1
	end
end)

GameData.Services.UIS.InputEnded:Connect(function(input)
	if not GameData.Drone then return end
	if input.KeyCode == Enum.KeyCode.W or input.KeyCode == Enum.KeyCode.S then
		GameData.MoveForward = 0
	elseif input.KeyCode == Enum.KeyCode.A or input.KeyCode == Enum.KeyCode.D then
		GameData.MoveRight = 0
	end
end)

GameData.Services.UIS.InputChanged:Connect(function(input, gpe)
	if not GameData.Drone or gpe then return end
	if input.UserInputType == Enum.UserInputType.MouseMovement then
		GameData.Yaw -= input.Delta.X * GameData.Sensitivity
		GameData.Pitch -= input.Delta.Y * GameData.Sensitivity
	end
	if input.KeyCode == Enum.KeyCode.Thumbstick1 then
		GameData.MoveRight = input.Position.X
		GameData.MoveForward = -input.Position.Y
	end
	if input.KeyCode == Enum.KeyCode.Thumbstick2 then
		GameData.Yaw -= input.Position.X * GameData.Sensitivity * 5
		GameData.Pitch -= input.Position.Y * GameData.Sensitivity * 5
	end
	GameData.Pitch = math.clamp(GameData.Pitch, -80, 80)
end)

GameData.Services.RunService.RenderStepped:Connect(function()
	if not GameData.Drone then return end
	local cf =
		CFrame.new(GameData.Camera.CFrame.Position) *
		CFrame.Angles(0, math.rad(GameData.Yaw), 0) *
		CFrame.Angles(math.rad(GameData.Pitch), 0, 0)
	local move =
		(cf.LookVector * GameData.MoveForward) +
		(cf.RightVector * GameData.MoveRight)
	GameData.Camera.CFrame = cf + (move * GameData.Speed)
end)

Sec.Player:Toggle({
    Title = "Drone Camera",
    Flag = "DroneCamera",
    Default = false,
    Callback = function(value)
        if value then
            GameData:EnableDrone()
        else
            GameData:DisableDrone()
        end
    end
})


