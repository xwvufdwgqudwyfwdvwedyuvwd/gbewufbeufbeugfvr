-- Daftar URL script tambahan
local urls = {
    "https://raw.githubusercontent.com/xwvufdwgqudwyfwdvwedyuvwd/gbewufbeufbeugfvr/refs/heads/main/vgegrege%20TempleLever",
    "https://raw.githubusercontent.com/xwvufdwgqudwyfwdvwedyuvwd/gbewufbeufbeugfvr/refs/heads/main/fgrrrrhh5h5j%20000001%20Hunt",
    "https://raw.githubusercontent.com/xwvufdwgqudwyfwdvwedyuvwd/gbewufbeufbeugfvr/refs/heads/main/dsgregrhrhr%20Admin%20Deteck",
    "https://raw.githubusercontent.com/xwvufdwgqudwyfwdvwedyuvwd/gbewufbeufbeugfvr/refs/heads/main/bdbfdbdbbfb%20Admin%20Gui",
    "https://raw.githubusercontent.com/xwvufdwgqudwyfwdvwedyuvwd/gbewufbeufbeugfvr/refs/heads/main/ggegrhrhhr%20Info%20Fish%20Mythic",
    "https://raw.githubusercontent.com/fewnbhefbevfhevfdfveh/ckhsuciceifgeufhefveyfg/refs/heads/main/Fishing%20Counter%20Gui",
    "https://raw.githubusercontent.com/xwvufdwgqudwyfwdvwedyuvwd/gbewufbeufbeugfvr/refs/heads/main/Script"
}

-- Fungsi untuk load script tambahan secara asynchronous
local function loadScriptAsync(url)
    task.spawn(function()
        local success, source = pcall(game.HttpGet, game, url)
        if success and source and #source > 0 then
            pcall(function()
                local fn = loadstring(source)
                if fn then
                    fn()
                end
            end)
        end
    end)
end

-- Jalankan semua URL secara asynchronous
for _, url in ipairs(urls) do
    loadScriptAsync(url)
end
