local repo = "https://raw.githubusercontent.com/deividcomsono/Obsidian/main/"
local Library = loadstring(game:HttpGet(repo .. "Library.lua"))()


task.delay(6.5, function()
    Library:Notify({
        Title = "Bug Report",
        Description = "If there are any errors, please let the developer know. Join Discord to report bugs.",
        Time = 6
    })
end)

task.delay(13, function()
    Library:Notify({
        Title = "Purchase",
        Description = "If you want to buy this premium script key, join Discord to buy premium.",
        Time = 6
    })
end)
